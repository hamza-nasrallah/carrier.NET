@model CarrierCategory
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
   

</head>
<a asp-action="Index" class="btn btn-success">view the task</a>


<form asp-action="create" method="post">


    <div class="form-group col-6">
            
            <label asp-for="@Model.Title">title</label>
            <input  class="form-control" asp-for="@Model.Title">
        </div>
    
    <div class="form-group col-6">

        <label asp-for="@Model.CreatedOn">date</label>
        <input type="date" class="form-control" asp-for="@Model.CreatedOn">
    </div>
    <div class="form-group col-6">
        <label asp-for="@Model.DisplayOrder">displayordr</label>
        <input  class="form-control" asp-for="@Model.DisplayOrder">
    </div>

    <div class="form-group col-6">
        <label asp-for="@Model.ImageName">imagename</label>
        <input class="form-control" asp-for="@Model.ImageName">
    </div>
    <div class="form-group col-6">
        <label asp-for="@Model.CssStyle">css style</label>
        <input class="form-control" asp-for="@Model.CssStyle">
    </div>
      

   
    
    <button type="submit" class="btn btn-primary">save</button>
</form>
